                    "sInfoFiltered": "(được lọc từ _MAX_ mục)",
                    "sInfoPostFix": "",
                    "sSearch": "Tìm kiếm: ",
                    "sUrl": "",
                    "oPaginate": {
                        "sFirst": "Đầu",
                        "sPrevious": "Trước",
                        "sNext": "Tiếp",
                        "sLast": "Cuối"
                    },
                },
                "pageLength": 15,
                buttons: [
                    {
                        extend: 'copy',
                        text: 'Sao chép',
                        className: 'btn btn-success btn-md'
                    },
                    {
                        extend: 'excel',
                        text: 'Xuất file excel',
                        className: 'btn btn-success btn-md',
                        filename: 'DanhSachHoiVien_' + d.getDate().toString() + '.' + d.getMonth().toString() + '.' + d.getFullYear().toString(),
                        title: 'DANH SÁCH HỘI VIÊN'
                    },
