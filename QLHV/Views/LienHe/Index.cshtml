@model List<QLHV.Models.ViewModels.LienHe.LienHeView>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="app">
    <div class="main-wrapper">
        <!-- Main Content -->
        <div class="main-content">
            <section class="section">
                <div class="section-header">
                    <h1>Thông tin liên hệ</h1>
                    <div class="section-header-breadcrumb">
                        <div class="breadcrumb-item active"><a href="#" style="color:rgba(40, 163, 40, 0.8); font-size:14px">Liên hệ</a></div>
                    </div>
                </div>
                <div class="section-body">
                    <div class="row">
                        @foreach (var it in Model)
                        {
                            <div class="col-12 col-md-6 col-lg-3">
                                <div class="card card-primary">
                                    <div class="card-header">
                                        <h4>@it.ten</h4>
                                    </div>
                                    <div class="card-body">
                                        <p>Email: <code>@it.email</code></p>
                                        <p>SĐT: <code>@it.SDT</code></p>
                                    </div>
                                </div>
                            </div>
                        }

                    </div>
                </div>

            </section>
        </div>
    </div>
</div>





