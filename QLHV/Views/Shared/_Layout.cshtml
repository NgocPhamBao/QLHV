    <script>
        $(document).ready(function () {
            var d = new Date();            
            var table = $('#example').DataTable({
                lengthChange: false,
                "deferRender": true,
                "language": {
                    "sProcessing": "Đang xử lý...",
                    "sLengthMenu": "Xem _MENU_ mục",
                    "sZeroRecords": "Không tìm thấy thông tin phù hợp",
